# Supabase
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54421
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# GitHub OAuth (create at https://github.com/settings/developers)
# Callback URL: http://localhost:54421/auth/v1/callback
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# 32-byte key, base64-encoded (used to encrypt stored GitHub tokens)
GITHUB_TOKEN_ENCRYPTION_KEY=base64-32-byte-key

# LLM Configuration (for AI-generated narratives)
# At least one provider API key should be set for platform-level LLM access
# The first configured provider becomes the primary/default
ANTHROPIC_API_KEY=your-anthropic-api-key
# ANTHROPIC_MODEL=claude-sonnet-4-20250514  # Optional: override default model

# Optional: Additional LLM providers
# OPENAI_API_KEY=your-openai-api-key
# OPENAI_MODEL=gpt-4o  # Optional: override default model

# GEMINI_API_KEY=your-gemini-api-key
# GEMINI_MODEL=gemini-2.0-flash  # Optional: override default model

# User API Key Encryption (required for BYOK - Bring Your Own Key)
# 32-byte key, base64-encoded (used to encrypt stored user LLM API keys)
# Generate with: openssl rand -base64 32
LLM_KEY_ENCRYPTION_SECRET=base64-32-byte-key-for-llm

# App
NEXT_PUBLIC_APP_URL=http://localhost:8108
