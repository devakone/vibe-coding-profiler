{
  "name": "@vibe-coding-profiler/db",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/database.types.mjs",
      "require": "./dist/database.types.js",
      "types": "./dist/database.types.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/database.types.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/database.types.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.14"
  },
  "devDependencies": {
    "@types/node": "^25",
    "tsup": "^8",
    "typescript": "^5"
  }
}
